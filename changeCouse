    def changeCouse(self):
        input = self.lineEdit.text()
        f = open('CourseInfo.txt', 'r')
        w = open('UpdatedCourseInfo.txt', 'w')
        data = f.readlines()
        updated_line = input.replace(' ', ',')
        data[10] = f"{updated_line} \n"
        w.writelines(data)
        f.close()
        w.close()

        #return input
